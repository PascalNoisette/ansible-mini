networks:
- config:
    ipv4.address: auto
    ipv4.firewall: "false"
    ipv4.nat: "true"
    ipv6.address: auto
    ipv6.nat: "true"
  name: lxdbr0
  type: bridge
  project: default
storage_pools:
- config:
    size: 24GB
    source: /var/snap/lxd/common/lxd/disks/default.img
  description: ""
  name: default
  driver: btrfs
- config:
    size: 48GB
    source: /var/snap/lxd/common/lxd/disks/first.img
  description: ""
  name: first
  driver: btrfs
- config:
    size: 24GB
    source: /var/snap/lxd/common/lxd/disks/jitsi.img
  description: ""
  name: jitsi
  driver: btrfs
- config:
    size: 24GB
    source: /var/snap/lxd/common/lxd/disks/mail.img
  description: ""
  name: mail
  driver: btrfs
- config:
    size: 24GB
    source: /var/snap/lxd/common/lxd/disks/second.img
  description: ""
  name: second
  driver: btrfs
- config:
    size: 24GB
    source: /var/snap/lxd/common/lxd/disks/hass.img
  description: ""
  name: hass
  driver: btrfs
- config:
    size: 32GB
    source: /var/snap/lxd/common/lxd/disks/tvhead.img
  description: ""
  name: tvhead
  driver: btrfs
profiles:
- config:
    boot.autostart: "true"
    linux.kernel_modules: ip_tables,ip6_tables,netlink_diag,nf_nat,overlay
    raw.lxc: |-
      "lxc.aa_profile=unconfined
      lxc.mount.auto=proc:rw sys:rw cgroup:rw
      lxc.cgroup.devices.allow = b 8:* rwm
      lxc.cgroup.devices.allow = c 189:* rwm
      lxc.cgroup.devices.allow = c 212:* rwm
      lxc.cgroup.devices.allow = c 116:* rwm
      lxc.cgroup.devices.allow = a
      lxc.hook.mount =
      lxc.cap.drop =
      lxc.hook.post-stop ="
    security.nesting: "true"
    security.privileged: "true"
    user.network-config: |
      version: 1
      config:
        - type: physical
          name: eth0
          subnets:
            - type: dhcp
              ipv4: true
        - type: physical
          name: eth1
          subnets:
            - type: dhcp
              ipv4: true
    user.user-data: |
      #cloud-config
      ssh_authorized_keys:
      - ssh-rsa  pascal
  description: Default LXD profile
  devices:
    certificates:
      path: /etc/letsencrypt/
      source: /etc/letsencrypt/
      type: disk
    dvbdemux:
      path: /dev/dvb/adapter0/demux0
      type: unix-char
    dvbdvr:
      path: /dev/dvb/adapter0/dvr0
      type: unix-char
    dvbfrontend:
      path: /dev/dvb/adapter0/frontend0
      type: unix-char
    dvbnet:
      path: /dev/dvb/adapter0/net0
      type: unix-char
    eth0:
      name: eth0
      network: lxdbr0
      type: nic
    eth1:
      nictype: macvlan
      parent: eno1
      type: nic
    gpu:
      type: gpu
    harddrive:
      path: /media/Saumon
      source: /media/Saumon
      type: disk
    home:
      path: /home/pascal/
      source: /home/pascal/
      type: disk
    root:
      path: /
      pool: default
      type: disk
    sound:
      path: /dev/snd
      source: /dev/snd
      type: disk
    ttyACM0:
      path: /dev/ttyACM0
      type: unix-char
    ttyUSB0:
      path: /dev/ttyUSB0
      type: unix-char
  name: default
- config:
    boot.autostart: "true"
    linux.kernel_modules: ip_tables,ip6_tables,netlink_diag,nf_nat,overlay
    raw.lxc: |-
      "lxc.aa_profile=unconfined
